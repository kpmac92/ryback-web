{
  "name": "ryback-web",
  "version": "1.0.0",
  "description": "A web app to view and manage recipies.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kpmac92/ryback-web.git"
  },
  "author": "Kevin Patrick",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kpmac92/ryback-web/issues"
  },
  "homepage": "https://github.com/kpmac92/ryback-web#readme",
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/jest-setup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testEnvironment": "jsdom"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@parcel/transformer-sass": "^2.8.0",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.1",
    "@types/jest": "^25.1.4",
    "babel-jest": "^25.2.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "jest": "^29.3.0",
    "jest-environment-jsdom": "^29.3.1",
    "parcel": "^2.8.0",
    "prettier": "^2.0.2",
    "process": "^0.11.10",
    "redux-mock-store": "^1.5.4"
  },
  "dependencies": {
    "node-sass": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.0"
  }
}
