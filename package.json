  "version": "1.0.0",
  "description": "A web app to view and manage recipies.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kpmac92/ryback-web.git"
  },
  "author": "Kevin Patrick",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kpmac92/ryback-web/issues"
  },
  "homepage": "https://github.com/kpmac92/ryback-web#readme",
  "jest": {
    "setupFilesAfterEnv": ["<rootDir>/src/jest-setup.js"],
